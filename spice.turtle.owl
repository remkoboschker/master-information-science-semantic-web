#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
<http://www.w3.org/2004/02/skos/core#prefLabel> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/remkoboschker/ontologies/spice#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          owl:inverseOf :isContainedIn ;
          rdfs:domain :Mixture ;
          rdfs:range :Food .


###  http://www.semanticweb.org/remkoboschker/ontologies/spice#isContainedIn
:isContainedIn rdf:type owl:ObjectProperty ;
               rdfs:domain :Food ;
               rdfs:range :Mixture .


###  http://www.semanticweb.org/remkoboschker/ontologies/spice#isFromRegion
:isFromRegion rdf:type owl:ObjectProperty ;
              owl:inverseOf :regionHasCuisine ;
              rdfs:domain :Cuisine .


###  http://www.semanticweb.org/remkoboschker/ontologies/spice#isUsedIn
:isUsedIn rdf:type owl:ObjectProperty ;
          owl:inverseOf :uses ;
          rdfs:domain :Food ;
          rdfs:range :Cuisine .


###  http://www.semanticweb.org/remkoboschker/ontologies/spice#regionHasCuisine
:regionHasCuisine rdf:type owl:ObjectProperty ;
                  rdfs:range :RegionalCuisine .


###  http://www.semanticweb.org/remkoboschker/ontologies/spice#uses
:uses rdf:type owl:ObjectProperty ;
      rdfs:domain :Cuisine ;
      rdfs:range :Food .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/remkoboschker/ontologies/spice#hasBotanicalName
:hasBotanicalName rdf:type owl:DatatypeProperty ;
                  rdfs:domain :AromaticVegetable ,
                              :Herb ,
                              :Spice ;
                  rdfs:range xsd:string .


###  http://www.semanticweb.org/remkoboschker/ontologies/spice#hasCommonName
:hasCommonName rdf:type owl:DatatypeProperty ;
               rdfs:domain :Cuisine ,
                           :Food ;
               rdfs:range xsd:string .


